<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY quest "http://obda.org/quest#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
    xml:base="http://www.sport.com/Fitness/Pattern#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:op= "http://www.sport.com/Fitness/Pattern#">
<owl:Ontology rdf:about="http://www.sport.com/Fitness/Pattern#"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <owl:ObjectProperty rdf:about="http://www.sport.com/Fitness/Pattern#hasCollection">
        <rdfs:domain rdf:resource="http://www.sport.com/Fitness/Pattern#Profile"/>
        <rdfs:range rdf:resource="http://www.sport.com/Fitness/Pattern#Collection"/>
    </owl:ObjectProperty>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="http://www.sport.com/Fitness/Pattern#hasLong">
        <rdfs:domain rdf:resource="http://www.sport.com/Fitness/Pattern#Profile"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty> 

    <owl:DatatypeProperty rdf:about="http://www.sport.com/Fitness/Pattern#hasLat">
        <rdfs:domain rdf:resource="http://www.sport.com/Fitness/Pattern#Profile"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty> 

    <owl:DatatypeProperty rdf:about="http://www.sport.com/Fitness/Pattern#hasHeight">
        <rdfs:domain rdf:resource="http://www.sport.com/Fitness/Pattern#Profile"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty> 

    <owl:DatatypeProperty rdf:about="http://www.sport.com/Fitness/Pattern#hasDistance">
        <rdfs:domain rdf:resource="http://www.sport.com/Fitness/Pattern#Profile"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty> 

    <owl:DatatypeProperty rdf:about="http://www.sport.com/Fitness/Pattern#hasDuration">
        <rdfs:domain rdf:resource="http://www.sport.com/Fitness/Pattern#Profile"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty> 

    <owl:DatatypeProperty rdf:about="http://www.sport.com/Fitness/Pattern#hasSpeed">
        <rdfs:domain rdf:resource="http://www.sport.com/Fitness/Pattern#Profile"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty> 

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
  
    <!-- http://www.sport.com/Fitness/Pattern#Critical -->

    <owl:Class rdf:about="http://www.sport.com/Fitness/Pattern#Critical">     
        <rdfs:subClassOf rdf:resource="http://www.sport.com/Fitness/Pattern#Profile"/>
    </owl:Class>
	
   <!-- http://www.sport.com/Fintess/Pattern#Profile -->

    <owl:Class rdf:about="http://www.sport.com/Fintess/Pattern#Profile">     
        <rdfs:subClassOf rdf:resource="http://www.sport.com/Fitness/Pattern#Collection"/>
    </owl:Class>
    
</rdf:RDF>
